<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlideBanner">
        <attr name="indicatorSize" format="dimension" />
        <attr name="auto_cycle" format="boolean" />
        <attr name="corner_radius" format="integer" />
        <attr name="period" format="integer" />
        <attr name="delay" format="integer" />
        <attr name="placeholder" format="reference" />
        <attr name="error_image" format="reference" />
        <attr name="selected_dot" format="reference" />
        <attr name="unselected_dot" format="reference" />
        <attr name="defaultTransformer" format="integer">
            <enum name="anti_clock_spin" value="1" />
            <enum name="carousel_effect" value="2" />
            <enum name="clock_spin" value="3"/>
            <enum name="cube_in_rotation" value="4"/>
            <enum name="cube_out_rotation" value="5"/>
            <enum name="depth" value="6"/>
            <enum name="fade_out" value="7"/>
            <enum name="horizontal_flip" value="8"/>
            <enum name="simple" value="9"/>
            <enum name="spinner" value="10"/>
            <enum name="vertical_flip" value="11"/>
            <enum name="vertical_shut" value="12"/>
            <enum name="zoom_out" value="13"/>
        </attr>
    </declare-styleable>
</resources>